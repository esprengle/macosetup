#!/usr/bin/env bash
# shellcheck disable=SC1090
# shellcheck disable=SC2034
# shellcheck disable=SC2154

# msx defaults
: "${atom_enabled:=false}"
#: "${atom_packages:=}"

# msx settings
msx_module_enabled="$atom_enabled"
msx_module_appcheck=(
  "/usr/local/bin/apm"
)

# msx main
msx_module_main()
{
  # apm install...
  for pkg in "${atom_packages[@]}"
  do
    echo apm install "$pkg"
  done
}
:
