#!/usr/bin/env bash
# shellcheck disable=SC1090
# shellcheck disable=SC2034
# shellcheck disable=SC2154

# defaults
msx_module_enabled="${atom_enabled:-false}"
msx_module_appcheck=(
  "/usr/local/bin/apm"
)

# module main function
msx_module_main()
{
  # apm install...
  for pkg in "${atom_packages[@]:-()}"
  do
    apm install "$pkg"
  done
}
:
